net="10.7.180."

priv="old.txt"

[ -s $priv ] || for (( i=0; i<255; i++ )); do echo 2; done >> $priv

state=( $(cat $priv) )

for i in {161..163}
do
  ip="$net$i"
  ping -c1 $ip &>>/dev/null
  res=$?

  #echo res $res
  #echo state ${state[$i]}
  if [ $res -ne ${state[$i]} ]
  then
    echo "$ip $res"
    state[$i]=$res
  fi
done

echo ${state[@]} > $priv
